{
  "name": "cloudstate-shopping",
  "version": "1.0.0",
  "description": "CloudState shopping cart stateless frontend",
  "keywords": [
    "serverless",
    "cloudstate",
    "crdt"
  ],
  "engineStrict": true,
  "engines": {
    "node": "~12"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "James Roper",
    "email": "jroper2@gmail.com",
    "url": "https://jazzy.id.au"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "@improbable-eng/grpc-web": "^0.12.0",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^4.0.0-alpha.38",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "cloudstate": "0.0.3",
    "clsx": "^1.0.4",
    "date-fns": "^2.10.0",
    "debug": "^4.1.1",
    "eject": "0.0.4",
    "express": "^4.17.1",
    "grpc": "^1.24.9",
    "grpc-web": "^1.0.7",
    "material-table": "^1.57.2",
    "mobx": "^5.5.2",
    "mobx-react": "^5.4.4",
    "mobx-react-router": "^4.0.5",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/compression": "0.0.33",
    "@types/es6-shim": "0.31.39",
    "@types/express": "4.0.33",
    "@types/google-protobuf": "^3.2.5",
    "@types/history": "4.5.0",
    "@types/node": "^10.12.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.0",
    "grpc-tools": "^1.9.0",
    "mobx-react-devtools": "^6.0.3",
    "react-hot-loader": "^4.12.21",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.0.1",
    "ts-protoc-gen": "0.12.0",
    "typescript": "3.7.4",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "clean": "rimraf public/build && rimraf src/_proto",
    "compile:protoc:cloudstate": "grpc_tools_node_protoc --proto_path=node_modules/cloudstate/proto --js_out=import_style=commonjs,binary:src/_proto --ts_out=src/_proto node_modules/cloudstate/proto/google/api/http.proto node_modules/cloudstate/proto/google/api/httpbody.proto node_modules/cloudstate/proto/google/api/annotations.proto node_modules/cloudstate/proto/cloudstate/entity_key.proto node_modules/cloudstate/proto/cloudstate/eventing.proto",
    "compile:protoc:shoppingcart": "grpc_tools_node_protoc --proto_path=. --js_out=import_style=commonjs,binary:src/_proto --ts_out=service=grpc-web:src/_proto -I node_modules/cloudstate/proto shoppingcart.proto",
    "precompile:protoc": "mkdirp src/_proto",
    "compile:protoc": "npm run compile:protoc:cloudstate && npm run compile:protoc:shoppingcart",
    "webpack": "webpack --config webpack.deploy.config.js",
    "webpack:dev": "webpack --config webpack.deploy.config.js --mode=development",
    "webpack:local_dev": "webpack --config webpack.deploy.config.js --mode=development --env.CART_SCHEME=http --env.CART_HOST=localhost --env.CART_PORT=8090",
    "build": "npm run compile:protoc && npm run webpack",
    "build:dev": "npm run compile:protoc && npm run webpack:dev",
    "build:local_dev": "npm run compile:protoc && npm run webpack:local_dev",
    "start": "node index.js"
  }
}
